{
  "package": {
    "name": "cma-template-jest",
    "version": "0.1.0",
    "private": true,
    "scripts": {
      "build-dev": "webpack --config webpack.dev.config.js",
      "build-prod": "webpack --config webpack.prod.config.js",
      "use-whistle-dev": "cd DevOps/whistle && npm run use-dev",
