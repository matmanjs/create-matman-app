    "dependencies": {
      "lodash": "^4.17.20"
    },
    "devDependencies": {
      "chai": "^4.2.0",
      "clean-webpack-plugin": "^3.0.0",
      "cross-env": "^7.0.2",
      "html-webpack-plugin": "^4.3.0",
      "istanbul-instrumenter-loader": "^3.0.1",
      "jest": "^26.4.2",
      "jest-matman-reporter": "^0.1.0",
      "matman-e2e-test": "^1.2.0",
      "webpack": "^4.44.1",
      "webpack-cli": "^3.3.12"
    }
  }
}